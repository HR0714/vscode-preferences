{
    "editor.fontSize": 15,
    "editor.links": false,
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "none",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.fontWeight": "200",
    "terminal.integrated.fontFamily": "Monaco",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.tips.enabled": false,
    "explorer.confirmDelete": false,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "editor.occurrencesHighlight":false,
    "editor.roundedSelection": false,
    "debug.toolBarLocation": "docked",
    "emmet.triggerExpansionOnTab": true,
    "editor.parameterHints.enabled": false,
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.renderLineHighlight": "gutter",
    "breadcrumbs.enabled": false,
    "editor.fontWeight": "bold",
    "editor.hover.enabled": false,
    "editor.renderWhitespace": "none",
    "workbench.editor.highlightModifiedTabs": true,
    "git.ignoreMissingGitWarning": true,
    "terminal.integrated.cursorWidth": 0,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.xlsx": "cweijan.officeViewer"
    },
    "security.workspace.trust.enabled": false,
    "editor.linkedEditing": true,
    "workbench.iconTheme": "office-material-icon-theme",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "workbench.editor.untitled.hint": "hidden",
    "explorer.confirmDragAndDrop": false,
    "vscode-office.openOutline": true,
    "settingsSync.ignoredExtensions": [
        "joelcrosby.one-dark-darker"
    ],
    "window.zoomLevel": -1,
    "workbench.colorTheme": "One Dark Darker",
    "editor.inlineSuggest.enabled": true,
}
